# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/express-backend

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=24h

# AI Configuration
TENSORFLOW_MEMORY_LIMIT=4096
AI_MODEL_PATH=./ai-models

# Logging
LOG_LEVEL=info

# API Configuration
API_PREFIX=/api
ENABLE_SWAGGER=true

# Security
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=15*60*1000
RATE_LIMIT_MAX=100
